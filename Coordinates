#include <iostream>
#include <math.h>
int main (int argc,char** argv)
{
  float x1,x2,x3,y1,y2,y3,AB,AC,BC;
  std::cout<<"Enter the coordinates of point A \n";
  std::cin>>x1>>y1;
  std::cout<<"Enter the coordinates of point B\n";
  std::cin>>x2>>y2;
  std::cout<<"Enter the coordinates of point C\n";
  std::cin>>x3>>y3;
  AB=sqrt(((x2-x1)*(x2-x1)+((y2-y1)*(y2-y1))));
  AC=sqrt(((x3-x1)*(x3-x1)+((y3-y1)*(y3-y1))));
  BC=sqrt(((x2-x3)*(x2-x3)+((y2-y3)*(y2-y3))));
  std::cout<<"the distance AB is"<<AB<<"\n";
  std::cout<<"the distance AC is"<<AC<<"\n";
  std::cout<<"the distance BC is"<<BC<<"\n";
}
